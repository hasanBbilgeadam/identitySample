
@{
    ViewData["Title"] = "ListUsers";
}

@model List<AppUserListVM>
<h1>ListUsers</h1>


<table class="table table-striped">

    <thead>
        <tr>
            <th>UserName</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>

        @{


            foreach (var item in Model)
            {
                <tr>
                
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                </tr>
            }
        }

    </tbody>
</table>
